{
  "name": "pocketflow-ts",
  "version": "1.0.0",
  "description": "A 300-line minimalist LLM framework for TypeScript/Node.js - Zero dependencies core, Agents, Task Decomposition, RAG, etc.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "type-check:examples": "tsc --project tsconfig.examples.json",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "setup": "node setup-env.js",
    "example:minimal": "ts-node examples/minimal/main.ts",
    "example:hello-world": "ts-node -r dotenv/config examples/hello-world/main.ts",
    "example:workflow": "ts-node -r dotenv/config examples/workflow/main.ts",
    "example:agent": "ts-node -r dotenv/config examples/agent/main.ts",
    "example:rag": "ts-node -r dotenv/config examples/rag/main.ts",
    "example:batch": "ts-node -r dotenv/config examples/batch/main.ts"
  },
  "keywords": [
    "llm",
    "ai",
    "agents",
    "workflow",
    "rag",
    "typescript",
    "nodejs",
    "minimalist"
  ],
  "author": "PocketFlow TS",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "24.0.15",
    "dotenv": "^17.2.0",
    "jest": "30.0.4",
    "openai": "5.10.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {},
  "peerDependencies": {
    "openai": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/pocketflow-ts.git"
  }
}